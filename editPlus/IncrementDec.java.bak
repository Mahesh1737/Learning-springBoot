class IncrementDec 
{
	public static void main(String[] args) 
	{
		int c= 5;
		int a= 9;
		System.out.println("Value of the a="+a+"\n Value of the c="+c);

//		System.out.println("Value of the a="+a+"\n Value of the c="+c);
                //     5     7      7     8     10    11 = 48
		int result = c++ + ++c + c++ + c++ + ++c + ++a;

		System.out.println("Value of the result is="+result);
	}
}
